{
	"$schema": "https://json.schemastore.org/tsconfig.json",
	"extends": "../../tsconfig.base.json",
	"compilerOptions": {
		"incremental": false,
		"composite": false,
		"module": "Node16",
		"moduleResolution": "node16",
		"types": [ "node" ],
		"noEmit": false,
		"outDir": "build",
		"sourceMap": true,
		"declaration": true,
		"declarationMap": true,
		"emitDeclarationOnly": false,
		"allowJs": true,
		"checkJs": false
	}
}
